    "migrations:delete": "sequelize-cli db:migrate:undo:all"
  },
  "keywords": [
    "node",
    "javascrip"
  ],
  "author": "Fernanda Ramírez <mframirezfuentes@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.2",
    "sequelize-cli": "^6.6.0"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "joi": "^17.7.0",
    "jsonwebtoken": "^9.0.0",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pg": "^8.8.0",
